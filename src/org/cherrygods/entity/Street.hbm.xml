<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.cherrygods.entity.Street" table="STREET" schema="SYSTEM">
        <id name="id">
            <column name="ID" sql-type="number(11)" precision="11"/>
        </id>
        <property name="name">
            <column name="NAME" sql-type="varchar2(50)" length="50" not-null="false"/>
        </property>
    </class>
</hibernate-mapping>