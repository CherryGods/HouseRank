<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.cherrygods.entity.Users" table="USERS" schema="SYSTEM">
        <id name="id">
            <column name="ID" sql-type="number(11)" precision="11"/>
        </id>
        <property name="name">
            <column name="NAME" sql-type="varchar2(50)" length="50" not-null="false"/>
        </property>
        <property name="password">
            <column name="PASSWORD" sql-type="varchar2(50)" length="50" not-null="false"/>
        </property>
        <property name="telephone">
            <column name="TELEPHONE" sql-type="varchar2(15)" length="15" not-null="false"/>
        </property>
        <property name="username">
            <column name="USERNAME" sql-type="varchar2(50)" length="50" not-null="false"/>
        </property>
        <property name="isadmin">
            <column name="ISADMIN" sql-type="varchar2(5)" length="5" not-null="false"/>
        </property>
    </class>
</hibernate-mapping>